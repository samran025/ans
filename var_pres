
run.sh~:#ansible-playbook -i ansible_hosts --extra-vars="env=${ENV} var1='var_run'" all.yml
ansible_hosts~:#localhost_qa ansible_host=localhost ansible_connection=local var1='run_inv'
run.sh:ansible-playbook -i ansible_hosts --extra-vars="env=${ENV} var1='var_run'" all.yml
ansible_hosts:localhost_qa ansible_host=localhost ansible_connection=local var1='run_inv'
